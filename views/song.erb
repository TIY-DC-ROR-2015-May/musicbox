<h2>About This Song</h2>
<table>
  <tbody>
  <% @songs do |song| %>
    <tr>
      <th>Artist</td>
      <td><%= song.artist %></td>
    </tr>
    <tr>
      <th>Title</th>
      <td><%= song.title %></td>
    </tr>
    <tr>
      <th>Suggested By</th>
      <td><%= song.suggester.name %></td>
    </tr>
    <tr>
      <th>Votes</th>
      <td><%= song.value %></td>
    </tr>
    <% end %>
  </tbody>
</table>